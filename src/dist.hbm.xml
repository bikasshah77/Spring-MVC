<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="max.controller.DistBean" table="dist10">
		<id name="dist_code" type="string">
			<column name="dist_code" />
			<generator class="native" />
		</id>

		<property name="dist_name" type="string" column="dist_name" length="45"
			not-null="true" />
		<property name="state_code" type="string" column="state_code"
			length="30" not-null="true" />

	</class>

</hibernate-mapping>